    "xstate": "^5.7.0"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.17.12",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "cypress": "^13.6.6",
    "eslint": "^8.56.0",
    "histoire": "^0.17.9",
    "sass": "^1.71.1",
    "typescript": "^5.2.2",
